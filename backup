if {#} != 1   echo "Form: backup <weekday>"   exit 65536endunset exitset dir /library/mike/{1}/prizmcreate {dir} >.null >&.nullset list        make backup prizm.notesset list {list} buffer.pas buffer.asm buffer.macrosset list {list} find.passet list {list} orca.pas orca.asm orca.macros orca.smacset list {list} pcommon.pas pcommon.asm pcommon.macrosset list {list} print.passet list {list} prism.pas prism.asm prism.macros prism.rezset list {list} run.pas run.asm run.macros run.smacfor i in {list}   newer {dir}/{i} {i}   if {Status} != 0      echo {i}      copy -c {i} {dir}/{i}   endend